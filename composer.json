{
    "name": "code-distortion/laravel-collection-macros",
    "description": "A set of useful Laravel collection macros - a spatie/laravel-collection-macros fork",
    "keywords": [
        "laravel-collection-macros"
    ],
    "homepage": "https://github.com/code-distortion/laravel-collection-macros",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Chandler",
            "email": "tim@code-distortion.net"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "amphp/parallel": "^0.2.5",
        "amphp/parallel-functions": "^0.1.3",
        "mockery/mockery": "^1.4.2",
        "orchestra/testbench": "^5.5|^6.5",
        "phpunit/phpunit": "^9.4.4",
        "symfony/stopwatch": "^5.2"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Spatie\\CollectionMacros\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\CollectionMacros\\Test\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/phpunit --exclude-group=ignore"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\CollectionMacros\\CollectionMacroServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
